{
  "name": "website",
  "author": "Pete O'Shea",
  "license": "ISC",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "test-all": "npm run lint && npm run format && npm run type-check && npm run test-coverage",
    "test-coverage": "jest --coverage",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "eslint",
      "prettier --write"
    ],
    "@(*.@(json|md|yml)|.*rc)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "micromark": "^2.11.4",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  }
}
