{
  "name": "website",
  "author": "Pete O'Shea",
  "license": "ISC",
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "test-all": "npm run lint && npm run format && npm run type-check && npm run test-coverage",
    "test-coverage": "jest --coverage",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "eslint",
      "prettier --write"
    ],
    "@(*.@(json|md|yml)|.*rc)": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "micromark": "^2.11.4",
    "next": "^12.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.3.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4"
  }
}
